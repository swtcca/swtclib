{
  "name": "swtc-api",
  "version": "1.1.29",
  "description": "swtc lib api",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/swtc-api.js",
  "jsdelivr": "dist/swtc-api.js",
  "browser": {
    "swtc-tum3": false,
    "tum3-eth-abi": false
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -fr esm cjs dist src package-lock.json node_modules",
    "prettier": "prettier",
    "pretty-quick": "pretty-quick",
    "prettier:check": "prettier -c tssrc/*.ts",
    "prettier:tabs": "prettier --use-tabs --write package.json tssrc/*.ts",
    "tsc": "tsc; tsc -p tsconfig.cjs.json; tsc -p tsconfig.esm.json",
    "build:dev": "tsc && webpack --mode development",
    "build": "tsc && webpack",
    "tslint": "tslint tssrc/*.ts",
    "test": "mocha \"test/test*.js\"",
    "test:api": "mocha \"test/test*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swtcca/swtclib.git"
  },
  "keywords": [
    "api",
    "swtc",
    "jingtum"
  ],
  "author": "Lospring Liu <lospringliu@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "git+https://github.com/swtcca/swtclib.git"
  },
  "homepage": "https://github.com/swtcca/swtclib",
  "dependencies": {
    "axios": "^0.19.0",
    "swtc-transaction": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^12.7.1",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "swtc-tum3": "^0.1.2",
    "tslint": "^5.19.0",
    "tum3-eth-abi": "^1.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  }
}
