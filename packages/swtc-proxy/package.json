{
  "name": "swtc-proxy",
  "version": "0.0.1",
  "description": "rest api to jingtum blockchain",
  "keywords": [
    "rest",
    "api",
    "proxy",
    "jingtum"
  ],
  "author": "Lospring Liu <lospringliu@gmail.com>",
  "homepage": "https://github.com/swtcca/swtclib",
  "license": "BSD-2-Clause",
  "main": "index.js",
  "module": "dist/index.esm.js",
  "directories": {
    "dist": "dist",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swtcca/swtclib.git"
  },
  "scripts": {
    "prettier": "prettier",
    "pretty-quick": "pretty-quick",
    "prettier:check": "prettier -c tssrc/*.ts",
    "prettier:tabs": "prettier --use-tabs --write package.json tssrc/*.ts",
    "tslint": "tslint tssrc/*.ts",
    "build": "rollup -c",
    "test": "mocha \"test/test*.js\"",
    "test:api": "mocha \"test/api*.js\"",
    "test:extended": "mocha \"test/*.js\"",
    "test:nyc": "nyc --reporter=html --reporter=text mocha test/test*.js"
  },
  "bugs": {
    "url": "https://github.com/swtcca/swtclib/issues"
  },
  "dependencies": {
    "koa": "^2.11.0",
    "swtc-lib": "^1.6.36"
  },
  "devDependencies": {
    "@types/node": "^12.7.1",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rollup": "^1.19.4",
    "rollup-plugin-typescript2": "^0.24.0",
    "tslint": "^5.19.0",
    "typescript": "^3.4.5"
  }
}
