  "devDependencies": {
    "@types/node": "^12.7.1",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tslint": "^5.19.0",
    "typescript": "^3.4.5"
  }
