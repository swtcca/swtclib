    "tslint": "tslint tssrc/*.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "jest": "^26.0.0",
    "prettier": "^1.19.1",
    "rollup": "^1.29.1"
  },
  "keywords": [
    "codec",
    "swtc",
    "jingtum",
    "address"
  ],
  "readmeFilename": "README.md",
  "engines": {
    "node": ">=8"
